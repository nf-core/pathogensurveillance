            },
            "sources": [
                {
                    "path": "trees/snp",
                    "pattern": "^(.+?)--(.+?)\\.treefile$",
                    "capture_groups": ["report_id", "reference_id"]
                }
            ]
        },
        "variants": {
            "name": "Filtered variants",
