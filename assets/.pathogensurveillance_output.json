{
    "source": "pathogensurveillance",
    "outputs": {
        "shared_genes": {
            "name": "Shared gene alignments",
            "description": "Aligned sequences for each gene shared amoung a group of organisms in FASTA format",
            "category": "Multigene phylogeny",
            "primary": true,
            "parsers": {
                "r": "ape::read.FASTA"
            },
            "sources": [
                {
                    "path": "shared_genes",
                    "pattern": "^(.+?)/(.+?)\\.aligned\\.fas$",
                    "capture_groups": ["method", "gene_id"]
                }
            ]
        },
        "aligned_reads_raw": {
            "name": "Unfiltered read alignments",
            "description": "Unfiltered read alignements to references in BAM format",
            "category": "Variant analysis",
            "primary": false,
            "sources": [
                {
                    "path": "aligned_reads",
                    "pattern": "^(.+?)--([^.]+?)\\.bam$",
                    "capture_groups": ["reference_id", "sample_id"]
                }
            ]
        },
        "aligned_reads": {
            "name": "Filtered read alignments",
            "description": "Quality filtered read alignements to references in BAM format",
            "category": "Variant analysis",
            "primary": true,
            "sources": [
                {
                    "path": "aligned_reads",
                    "pattern": "^(.+?)--(.+?)\\.formatted\\.bam$",
                    "capture_groups": ["reference_id", "sample_id"]
                }
            ]
        },
        "aligned_reads_index": {
            "name": "Read alignment indices",
            "description": "Index for the filtered BAM files produced by samtools index",
            "category": "Variant analysis",
            "primary": false,
            "sources": [
                {
                    "path": "aligned_reads",
                    "pattern": "^(.+?)--(.+?)\\.formatted\\.bam\\.csi$",
                    "capture_groups": ["reference_id", "sample_id"]
                }
            ]
        },
        "mark_duplicates_metrics": {
            "name": "MarkDuplicates output",
            "description": "Output from picard MarkDuplicates containing summary data tables",
            "category": "Variant analysis",
            "primary": true,
            "sources": [
                {
                    "path": "aligned_reads",
                    "pattern": "^(.+?)--(.+?)\\.formatted\\.MarkDuplicates\\.metrics\\.txt$",
                    "capture_groups": ["reference_id", "sample_id"]
                }
            ]
        },
